{
  "name": "@eventhorizon/exchange-bot-sdk",
  "version": "1.0.7",
  "description": "",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "types": "dist/mjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "prebuild": "npm run lint",
    "_clear": "rimraf dist/*",
    "lint": "eslint --fix src/**/*.ts",
    "format": "prettier --log-level warn --write \"src/**/*.ts\"",
    "test": "./scripts/test-unit.sh",
    "test:system": "./scripts/test-system.sh",
    "build": "npm run _clear && npm run build:cjs && npm run build:esm && /bin/bash ./fixup",
    "build:cjs": "tsc -p tsconfig-cjs.json",
    "build:esm": "tsc -p tsconfig.json",
    "prepare": "husky"
  },
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/gitlab": "^13.1.0",
    "@semantic-release/npm": "^12.0.1",
    "@types/assert": "^1.5.11",
    "@types/big.js": "^6.2.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.14.5",
    "@types/ws": "^8.5.14",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "axios": "^1.8.1",
    "centrifuge": "^5.3.4",
    "dotenv": "^16.4.7",
    "env-var": "^7.5.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-typescript": "^0.14.0",
    "husky": "^9.0.11",
    "mocha": "^11.1.0",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.7",
    "semantic-release": "^24.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "ws": "^8.18.1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@eventhorizon/exchange-api": "^1.0.15",
    "@eventhorizon/exchange-crypto": "^1.0.3",
    "big.js": "^6.2.2",
    "ethers": "^6.13.5"
  }
}
